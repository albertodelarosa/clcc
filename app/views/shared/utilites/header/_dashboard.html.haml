%nav#nav.navbar.navbar-default.navbar-static-top.navbar-chicanolegacy
  .container-fluid
    .navbar-header
      %button.navbar-toggle{"aria-controls" => "navbar", "aria-expanded" => "true", "data-target" => "#navbar", "data-toggle" => "collapse", :type => "button"}
        %span.sr-only Toggle navigation
        %span.icon-bar
        %span.icon-bar
        %span.icon-bar
      = link_to root_path, class: "navbar-brand" do
        = image_tag("logo_header.png", height: "50px")
    #navbar.navbar-collapse.collapse{"aria-expanded" => "true"}
      %ul.nav.navbar-nav
        %li
          %a{:href => "#about"} Test Action
        %li
          %a{:href => "#contact"} Test Action

      %ul.nav.navbar-nav
        - if user_signed_in?

          %li.dropdown
            %a.dropdown-toggle{"aria-expanded" => "false", "data-toggle" => "dropdown", :href => "#", :role => "button"}
              Account Settings
              %span.caret
            %ul.dropdown-menu{:role => "menu"}
              %li
                =link_to "edit information", edit_user_registration_path
              %li
                =link_to "edit password", edit_user_password_path
              %li.divider
              %li
                %a{:href => "#"} unlock instructions
          %li
            = link_to "Sign Out", destroy_user_session_path, method: :delete
        -else
          = render "shared/utilites/header/logins"

%header.masthead
  %h1.text-center
    Chicano Legacy Car &amp; Bike Club

