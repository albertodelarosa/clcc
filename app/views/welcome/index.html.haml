#myCarousel.carousel.slide{"data-ride" => "carousel"}
  / Indicators
  %ol.carousel-indicators
    %li.active{"data-slide-to" => "0", "data-target" => "#myCarousel"}
    %li{"data-slide-to" => "1", "data-target" => "#myCarousel"}
    %li{"data-slide-to" => "2", "data-target" => "#myCarousel"}
  .carousel-inner{:role => "listbox"}
    .item.active
      %img.intro{alt: "First slide", src: "/assets/2015_06_27.jpg", height: "500px"}
        .container
          .carousel-caption
            %h1 Come out and have a great time!
            %p
              Call 510.812.1903 for more info
    .item
      %img.intro{alt: "Second slide", src: "/assets/eastbay.jpg", height: "500"}
        .container
          .carousel-caption
            %h1 Never forgetting who we are, ORALE!
            %p Proudly representing Chicano Legacy Car Club. Â¡Simon que si!
    .item
      %img.intro{alt: "Third slide", src: "/assets/bikes.jpg", height: "500"}
        .container
          .carousel-caption
            %h1 Raza Park
            %p
              Showing off the bikes and having great time!
  %a.left.carousel-control{"data-slide" => "prev", :href => "#myCarousel", :role => "button"}
    %span.glyphicon.glyphicon-chevron-left{"aria-hidden" => "true"}
    %span.sr-only Previous
  %a.right.carousel-control{"data-slide" => "next", :href => "#myCarousel", :role => "button"}
    %span.glyphicon.glyphicon-chevron-right{"aria-hidden" => "true"}
    %span.sr-only Net

